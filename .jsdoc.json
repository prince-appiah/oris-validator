{
    "source":{
        "include":"./lib",
        "includePattern":".+\\.js(doc|x)?$",
        "exclude":".+\\test.js(doc|x)?$"
    },
    "sourceType":"module",
    "tags":{
        "allUnknownTags":true,
        "dictionaries":["jsdoc","closure"]
    },
    "templates":{
        "cleverLinks":false,
        "monoSpaceLinks":false
    },
    "opts":{
        "destination":"./docs/"
    }
}